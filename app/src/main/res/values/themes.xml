    <style name="Theme.DGECImovil" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/aC</item>
        <item name="colorPrimaryVariant">@color/aO</item>
        <item name="colorOnPrimary">@color/bb</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dC</item>
        <item name="colorSecondaryVariant">@color/dO</item>
        <item name="colorOnSecondary">@color/nn</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="fontFamily">@font/dgeci</item>
        <item name="android:fontFamily">@font/dgeci</item>
        <item name="toolbarStyle">@style/Widget.MaterialComponents.Toolbar.PrimarySurface</item>
    </style>
</resources>